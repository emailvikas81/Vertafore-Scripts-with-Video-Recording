package com.vertafore.plm.fermenter.etl;

import java.util.ArrayList;

import com.vertafore.plm.fermenter.etl.fixtures.DataDetail;
import com.vertafore.plm.fermenter.FixtureDefinition;

public class EtlFixtureDefinition extends FixtureDefinition {

	private String jobLocation;
	private String jobName;
	private String jobParams;
	private ArrayList<String[]> cleanup;
	private ArrayList<DataDetail> dataInserts;
	private ArrayList<DataDetail> validations;
	private ArrayList<String[]> postCleanup;
	
	
	
	public ArrayList<String[]> getPostCleanup() {
		return postCleanup;
	}
	public void setPostCleanup(ArrayList<String[]> postCleanup) {
		this.postCleanup = postCleanup;
	}
	public ArrayList<DataDetail> getValidations() {
		return validations;
	}
	public void setValidations(ArrayList<DataDetail> validations) {
		this.validations = validations;
	}
	public String getJobLocation() {
		return jobLocation;
	}
	public void setJobLocation(String jobLocation) {
		this.jobLocation = jobLocation;
	}
	public String getJobName() {
		return jobName;
	}
	public void setJobName(String jobName) {
		this.jobName = jobName;
	}
	public String getJobParams() {
		return jobParams;
	}
	public void setJobParams(String jobParams) {
		this.jobParams = jobParams;
	}
	public ArrayList<String[]> getCleanup() {
		return cleanup;
	}
	public void setCleanup(ArrayList<String[]> cleanup) {
		this.cleanup = cleanup;
	}
	public ArrayList<DataDetail> getDataInserts() {
		return dataInserts;
	}
	public void setDataInserts(ArrayList<DataDetail> dataInserts) {
		this.dataInserts = dataInserts;
	}

}
